{
  "name": "Teleport",
  "version": "0.8.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.9.12",
    "@openid/appauth": "^1.2.7",
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-env": "^0.0.12",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "bootstrap": "^4.4.1",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.1.0",
    "electron-localshortcut": "^3.2.1",
    "electron-store": "^5.1.0",
    "esm": "^3.2.25",
    "graphql": "^14.5.8",
    "jwt-decode": "^2.2.0",
    "menubar": "^7.1.0",
    "node-fetch": "^2.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-fade-in": "^0.1.8",
    "react-lottie": "^1.2.3",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-transition-group": "^4.3.0",
    "semantic-ui-react": "^0.88.2"
  },
  "main": "public/electron.js",
  "homepage": "../",
  "scripts": {
    "start": "PORT=3001 rescripts start",
    "build": "rescripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3001 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder --mac"
  },
  "author": {
    "name": "Teleport",
    "email": "contact@tlprt.io",
    "url": "https://www.tlprt.io"
  },
  "build": {
    "appId": "com.tlprt.teleport-mac",
    "productName": "Teleport",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "category": "public.app-category.productivity",
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "src/helpers/*",
      "src/graphql/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "afterSign": "scripts/notarize.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "electron": "^8.2.5",
    "electron-builder": "^21.2.0",
    "electron-notarize": "^0.3.0",
    "typescript": "^3.7.3",
    "wait-on": "^3.3.0"
  }
}
